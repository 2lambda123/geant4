macro creavec dimvex=13
 ve/cr temp(1) r 
 ve/cr ssse(1) r
 ve/cr eepr(1) r
 ve/cr nop(1) i
 ve/cr momb(1) r
 ve/cr enb(1) r
 ve/cr pdgpr(1) i
 ve/cr pdgtg(1) i
 ve/cr nevnt(1) i
 ve/cr freeN(1) r
 ve/cr freeD(1) r
 ve/cr clustP(1) r
 ve/cr rMed(1) r
 ve/cr solA(1) r
 *ve/cr angle(4) r 30 60 90 130
 * --- Loop over particles (1=p, 2=d, 3=t, 4=alpha) ---
 do i = 1 , 4
   if [i]=1 then
     ve/cr nptf(4) r 27 27 27 25
   elseif [i]=2 then
     ve/cr nptg(4) r 18 19 18 18
   elseif [i]=3 then
     ve/cr npth(4) r 15 16 16 15
   elseif [i]=4 then
     ve/cr npti(4) r 18 18 17 17
   endif
* --- Loop over angles (1=30, 2=60, 3=90, 4=130) ---
   do j = 1 , 4
     if [i]=1 then
       npt=nptf([j])
     elseif [i]=2 then
       npt=nptg([j])
     elseif [i]=3 then
       npt=npth([j])
     elseif [i]=4 then
       npt=npti([j])
     endif
     a  = angle([j])
     ve/cr t[i][a]([npt]) r
     ve/cr s[i][a]([npt]) r
     ve/cr d[i][a]([npt]) r
   enddo
 enddo
 ve/del nptf
 ve/del nptg
 ve/del npth
 ve/del npti
return
