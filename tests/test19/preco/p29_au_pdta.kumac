macro preco targ=^12!C file=au79118 np=79 nn=118 e=29 cs=1670 _
           runn=2000 next=3 pq=32-rb20-hp nzone=2 nvex=26 _
           dir=./
*
  hi/del *
  ve/del *
  ve/cr angle(4) r 30 60 90 130 
  exec creavec [nvex]
  do i = 35 , $sigma(34+[next])
   clo [i]
  enddo
*
  exec chips [targ] [file] [np] [nn] [e] [runn] [next] [dir]
*
  exec getdat [file] [e]
*
  chain -preco29
  hi/fil 35 [dir]chips_[file]_e[e]_1.hbook
  hrin 1
  chain preco29 //LUN35
  do i = 2 , [next]
    lun = 34 + [i]
    hi/fil [lun] [dir]chips_[file]_e[e]_[i].hbook
    hrin 1 ! 99999
    chain preco29 //LUN[lun]
  enddo
  cd //preco29
*
  nevts = $hinfo(1,'sum')
  hi/del 1
*
  upcol   = violet
  downcol = red
  hxmin =  3.
  hxmax =  32.
  kxmin =  0.
  kxmax =  32.
  hymin =  1.0001e-5
  hymax =  .06
  yfnam =  .03
  hzero =  0.1e-12
  xfnam =  24.
  hfbin =  1
  hnbin =  25
  ptsize = 0.24
  lgndsz = 0.35
  dangle = 7.5
  pi = 3.141593
  danrad = $sigma([pi]*[dangle]/180.)
  hlxmin = $sigma(log10([hxmin]))
  hlxmax = $sigma(log10([hxmax]))
  halfbn = $sigma(0.5*([hlxmax]-[hlxmin])/[hnbin])
*
  1d         11 'T?p! at  30 deg (MeV)' [hnbin] [hlxmin] [hlxmax]
  hi/copy 11 12 'T?p! at  60 deg (MeV)'
  hi/copy 11 13 'T?p! at  90 deg (MeV)'
  hi/copy 11 14 'T?p! at 130 deg (MeV)'
  hi/copy 11 21 'T?d! at  30 deg (MeV)'
  hi/copy 11 22 'T?d! at  60 deg (MeV)'
  hi/copy 11 23 'T?d! at  90 deg (MeV)'
  hi/copy 11 24 'T?d! at 130 deg (MeV)'
  hi/copy 11 31 'T?t! at  30 deg (MeV)'
  hi/copy 11 32 'T?t! at  60 deg (MeV)'
  hi/copy 11 33 'T?t! at  90 deg (MeV)'
  hi/copy 11 34 'T?t! at 130 deg (MeV)'
  hi/copy 11 41 'T?a! at  30 deg (MeV)'
  hi/copy 11 42 'T?a! at  60 deg (MeV)'
  hi/copy 11 43 'T?a! at  90 deg (MeV)'
  hi/copy 11 44 'T?a! at 130 deg (MeV)'
  hi/copy 11 81 'zero level'
  hi/copy 11 82 'dT in MeV'
  call hbarx(0)
*
* -- adjust zero levels, create dT histogram
  ve/cr zerol([hnbin]) r [hnbin]*[hzero]
  put/cont 81 zerol
  put/err  81 $sigma(0.8*zerol)
** ___ Reuse of "zerol" to read the bin mean T-values ___
  get/abs  82 zerol
  put/cont 82 $sigma(10.**(zerol+[halfbn])-10.**(zerol-[halfbn]))
  ve/del zerol
*
  cut 0 -
	 cut $1 nd=0.and.PDG=2212.or.(ns=0.and.nz=1.and.nn=0) | protons
  cut $2 nd=0.and.ns=0.and.nz=1.and.nn=1               | deuteron
  cut $3 nd=0.and.ns=0.and.nz=1.and.nn=2               | triton
  cut $4 nd=0.and.ns=0.and.nz=2.and.nn=2               | he4
*
  opt zfl
  swi z
  zone 2 2
* --- Loop over angles (secondaries are separate inside the loop) ---
  do i = 0 , 3
    k  = [i]+1
    k1 = [k]+10 | p
    k2 = [k]+20 | d
    k3 = [k]+30 | t
    k4 = [k]+40 | a
    a=angle([k])
    arad =  $sigma([pi]*[a]/180.)
    message 'i=',[i],', adeg=',[a],', arad=',[arad]
    cut [k1] $1.and.abs(atan2(sqrt(px*px+py*py),pz)-[arad])<[danrad]
    cut [k2] $2.and.abs(atan2(sqrt(px*px+py*py),pz)-[arad])<[danrad]
    cut [k3] $3.and.abs(atan2(sqrt(px*px+py*py),pz)-[arad])<[danrad]
    cut [k4] $4.and.abs(atan2(sqrt(px*px+py*py),pz)-[arad])<[danrad]
*    message 'cuts are defined k=',[k1],[k2],[k3],[k4],[k5],[k6]
    nt/pl 70.log10(E-m+0.001) $[k1] ! ! -[k1]
    nt/pl 70.log10(E-m+0.001) $[k2] ! ! -[k2]
    nt/pl 70.log10(E-m+0.001) $[k3] ! ! -[k3]
    nt/pl 70.log10(E-m+0.001) $[k4] ! ! -[k4]
    domega = $sigma(2.*[pi]*(cos([arad]-[danrad])-cos([arad]+[danrad])))
*    w = $sigma([cs]*1000000./[domega]/[nevts])
    w = $sigma([cs]/[domega]/[nevts])
*    message 'selection is done dom=',[domega],', w=',[w],', cs=',[cs]
    div [k1] 82 [k1]  1. 1.
    add [k1] 81 [k1] [w] 1.
    div [k2] 82 [k2]  1. 1.
    add [k2] 81 [k2] [w] 1.
    div [k3] 82 [k3]  1. 1.
    add [k3] 81 [k3] [w] 1.
    div [k4] 82 [k4]  1. 1.
    add [k4] 81 [k4] [w] 1.
*    message 'histograms are waighted and added'
  enddo
  pi/del ' '
* Draw & Create the file
  swi gz
* Create the output file only
*  swi z
*
*  opt vert
  size 24 18
  set xwin 0.0001
  set ywin 0.0001
  *title [targ]//'(p,f)X, E='//[E]//'MeV, [q]=30,60,90,130^0! (PRECO-New)'
  title [targ]//'(p,f)X, E='//[E]//'MeV, [q]=30,60,90,130^0! (CHIPS+QE)'
  clr
  zone 2 2
  set xval
  opt logy
  opt linx
  set dmod 1
  ***** M.K. ****
  opt date
  ***************
  opt tic
  opt nsta
  opt utit
  set errx 0.01
  set xlab 1.25
  ve/cr mrk(4) R 20 25 21 26
  ve/cr md(4) R 1 2 1 2
  ve/cr mas(4) R 938.272 1875.61 2809.25 3727.4 
  f1=proton
  f2=deuteron
  f3=triton
  f4=alpha
  title ' ' u
  exec ~/PTOOL/black
  set xval
  set yval 70
  ve/cr xm(1) R 14.
  ve/cr ym1(1) R .000448
  ve/cr ym2(1) R .000224
  ve/cr ym3(1) R .000112
  ve/cr ym4(1) R .000056
* --- Loop over pictures for each secondary ---
  do j = 1 , 4
    if [j] = 2 THEN
      set xval 70
    endif
    if [j] = 3 THEN
      set xval
      set yval
      hymin=1.001e-6
      hymax=.006
      yfnam=.003
    endif
    if [j] = 4 THEN
      set xval 70
    endif
    null [kxmin] [kxmax] [hymin] [hymax]
    if [j] = 1 THEN
      h/symbols xm ym1 1 20 [ptsize]
      h/symbols xm ym2 1 25 [ptsize]
      h/symbols xm ym3 1 21 [ptsize]
      h/symbols xm ym4 1 26 [ptsize]
      itx 17. .0003840 30
      itx 17. .0001920 60
      itx 17. .0000960 90
      itx 17. .0000480 130
    endif
    if [j] = 4 then
      exec ~/PTOOL/black
      *atitle 'k = (p+T)/2 (MeV)       ^!' ' '
      atitle 'T (MeV)       ^!' ' '
    endif
    title ' '
    itx [xfnam] [yfnam] [f[j]]
    if [j] = 1 then
      exec ~/PTOOL/black
      atitle ' ' 'd[s]/pdEd[W] (mb MeV^-2! sr^-1!) ^!'
      exec paramline 0.2 0.2
    endif
*    exec ~/PTOOL/snotes 144. 1.0 '1.0' 0. [lgndsz] -40
*    exec ~/PTOOL/snotes 144. 0.1 '0.1' 0. [lgndsz] -40
    m = mas([j])
    exec ~/PTOOL/black
    do i = 0 , 3
      k    = [i]+1
      mk=mrk([k])
      mod=md([k])
      a=angle([k])
      set lwid 7.
**      fun/pl .11*EXP(-x/25) 30 250 S
      ve/copy $sigma(sqrt(t[j][a]*(2.*[m]+t[j][a]))) p[j][a]
      *h/errors $sigma(0.5*(p[j][a]+t[j][a])) $sigma(s[j][a]/p[j][a])
      h/errors t[j][a] $sigma(s[j][a]/p[j][a]) _
               ?  $sigma(d[j][a]/p[j][a]) 25 [mk] [ptsize] 0
*************** Temporary **************
*      ve/re abspr,con p29_au_[j]_[a]_lepr.dat
*      ve/cop $SIGMA(con*.45) conp
*      put/cont 6[i] conp
***********************************^^END
      exec hsolid [j][k] [hfbin] [hnbin] [downcol] [m] [mod]
    enddo
  enddo
*
  exec ~/PTOOL/black
  set xwin
  set ywin
  set xval
  set yval
  set errx
  set xlab
*
  exec prieps p_[e]_[file]_pdta
  if [runn] = 0 then
    wait ' ' 2
***    shell lpr -P[pq] preco_[file]_e[e].ps
***    shell xprint -P[pq] preco_[file]_e[e].ps
  endif
*
stop:
*
return
