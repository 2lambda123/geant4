macro getdat file=al1314 e=400000
*
  *ve/cr angle(5) r 70 90 118 137 160
  * Ta is hardwired, rename the files according to [file]
  do i = 1 , 5
    a  = angle([i])
    do j = 1 , 6
      if [j] = 1 then
        ve/re t[j][a],s[j][a],d[j][a] p400ta_p[a].dat
      endif
      if [j] = 2 then
        ve/re t[j][a],s[j][a],d[j][a] p400ta_d[a].dat
      endif
      if [j] = 3 then
        ve/re t[j][a],s[j][a],d[j][a] p400ta_t[a].dat
      endif
      if [j] = 4 then
        ve/re t[j][a],s[j][a],d[j][a] p400ta_h[a].dat
      endif
      if [j] = 5 then
        ve/re t[j][a],s[j][a],d[j][a] p400ta_pip[a].dat
      endif
      if [j] = 6 then
        ve/re t[j][a],s[j][a],d[j][a] p400ta_pim[a].dat
      endif
*      ve/cop $SIGMA(s[j][a]/20.) d[j][a]
    enddo
  enddo
return
