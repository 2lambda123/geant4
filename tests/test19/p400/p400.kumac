*
macro preco targ=^12!C file=c0606 np=6 nn=6 e=90 cs=999 _
           runn=2000 next=1 pq=32-rb20-hp nzone=2 nvex=26 _
           dir=./
*
  hi/del *
  ve/del *
  ve/cr angle(5) r 70 90 118 137 160
  exec creavec [nvex]
  do i = 35 , $sigma(34+[next])
   clo [i]
  enddo
  awt=[np]+[nn]
*
  exec chips [targ] [file] [np] [nn] [e] [runn] [next] [dir]
*
  exec getdat [file] [e]
*
  chain -p400
  hi/fil 35 [dir]chips_[file]_e[e]_1.hbook
  hrin 1
  chain p400 //LUN35
  do i = 2 , [next]
    lun = 34 + [i]
    hi/fil [lun] [dir]chips_[file]_e[e]_[i].hbook
    hrin 1 ! 99999
    chain p400 //LUN[lun]
  enddo
  cd //p400
*
  nevts = $hinfo(1,'sum')
  hi/del 1
*
  upcol   = violet
  downcol = red
  hxmin =  3.
  hxmax =  1300.
  kxmin =  0.
  kxmax =  1300.
  hymin =  1.0001e-9
  if [np] = 3 then
    hymax =  .0004
    yfnam =  2.e-9
  elseif [np] = 4 then
    hymax =  .0005
    yfnam =  2.e-9
  elseif [np] = 6 then
    hymax =  .0006
    yfnam =  2.e-9
  elseif [np] = 13 then
    hymax =  .002
    yfnam =  2.e-9
  elseif [np] = 29 then
    hymax =  .003
    yfnam =  2.e-9
  elseif [np] = 73 then
    hymax =  .009
    yfnam =  2.e-9
  endif
  hzero =  0.1e-12
* with yfman above defines the particle name position
  xfnam =  100.
  hfbin =  1
  hnbin =  50
  ptsize = 0.24
  lgndsz = 0.35
  dangle = 9.
  pi = 3.14159265
  *Alias/del paperX
  *Alias/cr  paperX '32.0' 
  *Alias/del paperY
  *Alias/cr  paperY '24.0' 
  *exec ~/PTOOL/reset
  *exec ~/PTOOL/setpq
  danrad = $sigma([pi]*[dangle]/180.)
  hlxmin = $sigma(log10([hxmin]))
  hlxmax = $sigma(log10([hxmax]))
  halfbn = $sigma(0.5*([hlxmax]-[hlxmin])/[hnbin])
*
  opt nzfl
  swi g
*
  1d         11 'T?p! at  70 deg (MeV)' [hnbin] [hlxmin] [hlxmax]
  hi/copy 11 12 'T?p! at  90 deg (MeV)'
  hi/copy 11 13 'T?p! at 118 deg (MeV)'
  hi/copy 11 14 'T?p! at 137 deg (MeV)'
  hi/copy 11 15 'T?p! at 160 deg (MeV)'
  hi/copy 11 21 'T?d! at  70 deg (MeV)'
  hi/copy 11 22 'T?d! at  90 deg (MeV)'
  hi/copy 11 23 'T?d! at 118 deg (MeV)'
  hi/copy 11 24 'T?d! at 137 deg (MeV)'
  hi/copy 11 25 'T?d! at 160 deg (MeV)'
  hi/copy 11 31 'T?t! at  70 deg (MeV)'
  hi/copy 11 32 'T?t! at  90 deg (MeV)'
  hi/copy 11 33 'T?t! at 118 deg (MeV)'
  hi/copy 11 34 'T?t! at 137 deg (MeV)'
  hi/copy 11 35 'T?t! at 160 deg (MeV)'
  hi/copy 11 41 'T?h! at  70 deg (MeV)'
  hi/copy 11 42 'T?h! at  90 deg (MeV)'
  hi/copy 11 43 'T?h! at 118 deg (MeV)'
  hi/copy 11 44 'T?h! at 137 deg (MeV)'
  hi/copy 11 45 'T?h! at 160 deg (MeV)'
  hi/copy 11 51 'T?[p]+! at  70 deg (MeV)'
  hi/copy 11 52 'T?[p]+! at  90 deg (MeV)'
  hi/copy 11 53 'T?[p]+! at 118 deg (MeV)'
  hi/copy 11 54 'T?[p]+! at 137 deg (MeV)'
  hi/copy 11 55 'T?[p]+! at 160 deg (MeV)'
  hi/copy 11 61 'T?[p]-! at  70 deg (MeV)'
  hi/copy 11 62 'T?[p]-! at  90 deg (MeV)'
  hi/copy 11 63 'T?[p]-! at 118 deg (MeV)'
  hi/copy 11 64 'T?[p]-! at 137 deg (MeV)'
  hi/copy 11 65 'T?[p]-! at 170 deg (MeV)'
  *hi/copy 11 71 'T?a! at  70 deg (MeV)'
  *hi/copy 11 72 'T?a! at  90 deg (MeV)'
  *hi/copy 11 73 'T?a! at 118 deg (MeV)'
  *hi/copy 11 74 'T?a! at 137 deg (MeV)'
  *hi/copy 11 75 'T?a! at 160 deg (MeV)'
  hi/copy 11 81 'zero level'
  hi/copy 11 82 'dT in MeV'
  call hbarx(0)
*
* -- adjust zero levels, create dT histogram
  ve/cr zerol([hnbin]) r [hnbin]*[hzero]
  put/cont 81 zerol
  put/err  81 $sigma(0.8*zerol)
** ___ Reuse of "zerol" to read the bin mean T-values ___
  get/abs  82 zerol
  put/cont 82 $sigma(10.**(zerol+[halfbn])-10.**(zerol-[halfbn]))
  ve/del zerol
*
  cut 0 -
  cut $1 nd=0.and.PDG=2212.or.(ns=0.and.nz=1.and.nn=0) | protons
  cut $2 nd=0.and.ns=0.and.nz=1.and.nn=1               | deuteron
  cut $3 nd=0.and.ns=0.and.nz=1.and.nn=2               | triton
  cut $4 nd=0.and.ns=0.and.nz=2.and.nn=1               | he3
  cut $5 nd=0.and.PDG=211                              | pi+
  cut $6 nd=0.and.PDG=-211                             | pi-
  *cut $7 nd=0.and.ns=0.and.nz=2.and.nn=2               | he4
  zone 3 2
  do i = 0 , 4
    k  = [i]+1
    k1 = [k]+10 | p
    k2 = [k]+20 | d
    k3 = [k]+30 | t
    k4 = [k]+40 | He3
    k5 = [k]+50 | pi+
    k6 = [k]+60 | pi-
    a=angle([k])
    arad =  $sigma([pi]*[a]/180.)
    message 'i=',[i],', adeg=',[a],', arad=',[arad]
    cut [k1] $1.and.abs(atan2(sqrt(px*px+py*py),pz)-[arad])<[danrad]
    cut [k2] $2.and.abs(atan2(sqrt(px*px+py*py),pz)-[arad])<[danrad]
    cut [k3] $3.and.abs(atan2(sqrt(px*px+py*py),pz)-[arad])<[danrad]
    cut [k4] $4.and.abs(atan2(sqrt(px*px+py*py),pz)-[arad])<[danrad]
    cut [k5] $5.and.abs(atan2(sqrt(px*px+py*py),pz)-[arad])<[danrad]
    cut [k6] $6.and.abs(atan2(sqrt(px*px+py*py),pz)-[arad])<[danrad]
*    message 'cuts are defined k=',[k1],[k2],[k3],[k4],[k5],[k6]
    nt/pl 70.log10(E-m+0.001) $[k1] ! ! -[k1]
    nt/pl 70.log10(E-m+0.001) $[k2] ! ! -[k2]
    nt/pl 70.log10(E-m+0.001) $[k3] ! ! -[k3]
    nt/pl 70.log10(E-m+0.001) $[k4] ! ! -[k4]
    nt/pl 70.log10(E-m+0.001) $[k5] ! ! -[k5]
    nt/pl 70.log10(E-m+0.001) $[k6] ! ! -[k6]
    domega = $sigma(2.*[pi]*(cos([arad]-[danrad])-cos([arad]+[danrad])))
    w = $sigma([cs]/[domega]/[nevts])
*    message 'selection is done dom=',[domega],', w=',[w],', cs=',[cs]
    div [k1] 82 [k1]  1. 1.
    add [k1] 81 [k1] [w] 1.
    div [k2] 82 [k2]  1. 1.
    add [k2] 81 [k2] [w] 1.
    div [k3] 82 [k3]  1. 1.
    add [k3] 81 [k3] [w] 1.
    div [k4] 82 [k4]  1. 1.
    add [k4] 81 [k4] [w] 1.
    div [k5] 82 [k5]  1. 1.
    add [k5] 81 [k5] [w] 1.
    div [k6] 82 [k6]  1. 1.
    add [k6] 81 [k6] [w] 1.
*    message 'histograms are waighted and added'
  enddo
  pi/del ' '
*
  opt zfl
* Draw & Create the file
  swi gz
* Create the output file only
*  swi z
*
*  opt vert
  size 24 18
  set xwin 0.0001
  set ywin 0.0001
*  model=lowen
  model=chips
*  model=qgsp
*  model=ftfp
*  model=preco
*  model=berti
*  model=binar
*  model=lepro
  title [targ]//'(p,f)X,E='//[E]//'MeV,[q]=70,90,118,137,160^0!('//[model]//')'
  clr
  zone 3 2
  set xval
  opt logy
  opt linx
  set dmod 1
  ***** M.K. ****
  opt date
  ***************
  opt tic
  opt nsta
  opt utit
  set errx 0.01
  set xlab 1.25
  ve/cr mrk(5) R 20 25 22 24 21
  ve/cr md(5) R 1 2 1 2 1
*  ve/cr mas(6) R 939.565 938.272 1875.61 2809.25 2809.23 3727.4 
  ve/cr mas(6) R 938.272 1875.61 2809.25 2809.23 139.57 139.57
  f1=proton
  f2=deuteron
  f3=triton
  f4=He3
  f5=pi+
  f6=pi-
  title ' ' u
  exec ~/PTOOL/black
  set xval
  set yval 70
  xt=150
* Legend of angles
  ve/cr xm(1) R 110.
  ve/cr ym1(1) R 9.0e-9
  ve/cr ym2(1) R 2.7e-8
  ve/cr ym3(1) R 8.1e-8
  ve/cr ym4(1) R 2.4e-7
  ve/cr ym5(1) R 7.2e-7
* Vector of the center bins in Kinetic Energy for all spectra (the same)
  ve/cr lke([hnbin])
  get/absc 11 lke
* LOOP over secondary fragments
  do j = 1 , 6
    if [j] = 2 THEN
      set xval 70
    endif
    if [j] = 4 THEN
      set xval
      set yval
      hymin=1.001e-9
* This is duplication for the second row
*      if [np] = 3 then
*        hymax =  .0004
*        yfnam =  3.e-9
*      elseif [np] = 4 then
*        hymax =  .0005
*        yfnam =  3.e-9
*      elseif [np] = 6 then
*        hymax =  .0006
*        yfnam =  3.e-9
*      elseif [np] = 13 then
*        hymax =  .002
*        yfnam =  3.e-9
*      elseif [np] = 29 then
*        hymax =  .003
*        yfnam =  3.e-9
*      elseif [np] = 73 then
*        hymax =  .009999
*        yfnam =  3.e-9
*      endif
    endif
    if [j] = 5 THEN
      set xval 70
    endif
    null [kxmin] [kxmax] [hymin] [hymax]
    if [j] = 1 THEN
      *ve/cr mrk(9) R 20 25 22 24 21 26 29 28 23
      h/symbols xm ym1 1 20 [ptsize]
      h/symbols xm ym2 1 25 [ptsize]
      h/symbols xm ym3 1 22 [ptsize]
      h/symbols xm ym4 1 24 [ptsize]
      h/symbols xm ym5 1 21 [ptsize]
      itx [xt] 6.0e-9 70
      itx [xt] 1.8e-8 90
      itx [xt] 5.4e-8 118
      itx [xt] 1.6e-7 137
      itx [xt] 4.8e-7 160
    endif
    if [j] = 9 then
      exec ~/PTOOL/black
      *atitle 'k = (p+T)/2 (MeV)       ^!' ' '
      atitle 'T (MeV)       ^!' ' '
    endif
    title ' '
    itx [xfnam] [yfnam] [f[j]]
    if [j] = 1 then
      exec ~/PTOOL/black
      atitle ' ' 'd[s]/pdEd[W] (mb MeV^-2! sr^-1!) ^!'
      exec paramline 0.2 0.2
    endif
*    exec ~/PTOOL/snotes 144. 1.0 '1.0' 0. [lgndsz] -40
*    exec ~/PTOOL/snotes 144. 0.1 '0.1' 0. [lgndsz] -40
    m = mas([j])
    exec ~/PTOOL/black
*   LOOP over angles
    do i = 0 , 4
      k    = [i]+1
      jk   = [j]*10+[k]
      mk=mrk([k])
      mod=md([k])
      a=angle([k])
      set lwid 7.
**      fun/pl .11*EXP(-x/25) 30 250 S
*:*   For non-invariant ===================================
*      ve/copy $sigma(sqrt(t[j][a]*(2.*[m]+t[j][a]))) p[j][a]
*      *h/errors $sigma(0.5*(p[j][a]+t[j][a])) $sigma(s[j][a]/p[j][a])
*      h/errors t[j][a] $sigma(s[j][a]/p[j][a]) _
*               ?  $sigma(d[j][a]/p[j][a]) 25 [mk] [ptsize] 0
*:*   For invariant /A GeV-2(GeV)===================================
      h/errors $sigma(t[j][a]*1000.) $sigma(s[j][a]*[awt]/1000000) ? _
               $sigma(d[j][a]*[awt]/1000000) 25 [mk] [ptsize] 0
*      if [j] < 3 then
*       mess 'data: h='//[j]//', ang='//[i]
*       ve/pri $sigma(s[j][a]/p[j][a])
*      endif
      exec hsolid [j][k] [hfbin] [hnbin] [downcol] [m] [mod]
*     prepare the vector corresponding to the histogram
      ve/cr v[jk]([hnbin])
      get/cont [jk] v[jk]
    enddo
*   p(j=1),d(j=2),t(j=3),He3(j=4),pi+(j=5),pi-(j=6)
    ve/wri lke,v[j]1,v[j]2,v[j]3,v[j]4,v[j]5 _
    p_[e]_[file]_[j]_[model].v '1X,F9.6,5E10.3' 'OC'
  enddo
*
  exec ~/PTOOL/black
  set xwin
  set ywin
  set xval
  set yval
  set errx
  set xlab
*
  exec prieps p_[e]_[file]_npdt_[model]
  if [runn] = 0 then
    wait ' ' 2
***    shell lpr -P[pq] preco_[file]_e[e]_[model].ps
***    shell xprint -P[pq] preco_[file]_e[e]_[model].ps
  endif
*
stop:
*
return
