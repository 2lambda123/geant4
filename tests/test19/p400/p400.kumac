macro run1 f=5 n=1 z=73 que=32-rb20-hp
 nEvts = 2000 * [f]
 if [z] = 3 then	
  exec preco ^7!Li li0304 3 4 400000 169 [nEvts] [n] [que] 2 81 | 169 mb is reaction CS
 elseif [z] = 4 then	
  exec preco ^9!Be be0405 4 5 400000 206 [nEvts] [n] [que] 2 81 | 206 mb is reaction CS
 elseif [z] = 6 then	
  exec preco ^12!C c0606 6 6 400000 254 [nEvts] [n] [que] 2 81 | 254 mb is reaction CS
 elseif [z] = 13 then	
  exec preco ^27!Al al1314 13 14 400000 446 [nEvts] [n] [que] 2 81 | 446 mb is reaction CS
 elseif [z] = 29 then	
  exec preco ^63!Al cu2934 29 34 400000 812 [nEvts] [n] [que] 2 81 | 812 mb is reaction CS
 elseif [z] = 73 then
  exec preco ^181!Ta ta73108 73 108 400000 1689 [nEvts] [n] [que] 2 81 | 1689 mb is CS
 endif
return
