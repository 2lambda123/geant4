macro hsolid id=66 hf=1 hn=60 col=black m=939.567 mod=1
  set dmod [mod]
  nb = $sigma([hn]-[hf]+1)
  ve/cr wx([nb]) r
  ve/cr wy([nb]) r
  ve/cr wd([nb]) r
  get/abs  [id]([hf]:[hn]) wx
  get/cont [id]([hf]:[hn]) wy
  get/err  [id]([hf]:[hn]) wd
  ve/copy $sigma(10.**wx) wt
  ve/copy $sigma(sqrt(wt*(2.*[m]+wt))) wp
*  if [m] < 1000 then
*    mess 'fit: m='//[m]//', id='//[id]
*    ve/pri $sigma(wy/wp)
*  endif
  exec ~/PTOOL/white 8
  set lwid 5
  graph [nb] wt $sigma(wy/wp) l
  exec ~/PTOOL/[col]
  set lwid 5
  graph [nb] wt $sigma(wy/wp) l
  exec ~/PTOOL/black
  set dmod 3
  graph [nb] wt $sigma(wy/wp) l
  set lwid 5
  set dmod 1
***  h/errors wt $sigma(wy/wp) $sigma(0.*wt) $sigma(wd/wp) [nb] 1 .01
  ve/del wx,wy,wd,wt,wp
return
