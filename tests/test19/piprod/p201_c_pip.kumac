macro getdat file=c0606 e=201
*
* -- in the pipro_[file]_h_e[e]_ang.vexp the pip spectra are given in:
* --  mbarn MeV^-1 Omega^-1, function of T in MeV 
* --- Loop over angles ---
  ve/cr shift(6) r 5.77 5.05 4.32 2.88 1.44 0.
  do i = 1 , 6
    a  = angle([i])
*    do j = 1 , 4
    j=1
    sh = shift([i])
*      if [j] = 1 then
        ve/re t[j][a],x[j][a],e[j][a] p[e]_c_pip_[a].dat
        ve/cop $SIGMA((x[j][a]-[sh])/4780.) s[j][a]
        ve/cop $SIGMA(e[j][a]/4780.) d[j][a]
*      elseif [j] = 2 then
*        ve/re t[j][a],s[j][a],d[j][a] p_e[e]_[file]_d_[a].dat
*      elseif [j] = 3 then
*        ve/re t[j][a],s[j][a],d[j][a] p_e[e]_[file]_t_[a].dat
*      elseif [j] = 4 then
*        ve/re t[j][a],s[j][a],d[j][a] p_e[e]_[file]_he4_[a].dat
*      endif
*    enddo
  enddo
return
