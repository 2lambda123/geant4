macro pipro targ=^12!C file=c0606 np=6 nn=6 e=201 cs=208 _
            runn=2000 next=3 pq=32-rb20-hp nzone=2 nvex=13 dir=./
*
  hi/del *
  ve/del *
  ve/cr angle(6) r 30 45 60 90 116 151 
  exec creavec [nvex]
  do i = 35 , $sigma(34+[next])
   clo [i]
  enddo
*
  exec chips [targ] [file] [np] [nn] [e] [runn] [next] [dir]
*
  exec getdat [file] [e]
*
  chain -pipr201c
  hi/fil 35 [dir]chips_[file]_e[e]_1.hbook
  hrin 1
  chain pipr201c //LUN35
  do i = 2 , [next]
    lun = 34 + [i]
    hi/fil [lun] [dir]chips_[file]_e[e]_[i].hbook
    hrin 1 ! 99999
    chain pipr201c //LUN[lun]
  enddo
  cd //pipr201c
*
  nevts = $hinfo(1,'sum')
  hi/del 1
*
  upcol   = violet
  downcol = red
  hxmin =  3.
  hxmax =  63.
  kxmin =  0.
  kxmax =  63.
  hymin =  2.e-8
  hymax =  2.e-5
  yfnam =  .2
  hzero =  0.1e-12
  xfnam =  24.
  hfbin =  1
  hnbin =  25
  ptsize = 0.24
  lgndsz = 0.35
  dangle = 7.5
  pi = 3.141593
  danrad = $sigma([pi]*[dangle]/180.)
  hlxmin = $sigma(log10([hxmin]))
  hlxmax = $sigma(log10([hxmax]))
  halfbn = $sigma(0.5*([hlxmax]-[hlxmin])/[hnbin])
*
  opt nzfl
  swi g
*
  1d         11 'T?p! at  30 deg (MeV)' [hnbin] [hlxmin] [hlxmax]
  hi/copy 11 12 'T?p! at  45 deg (MeV)'
  hi/copy 11 13 'T?p! at  60 deg (MeV)'
  hi/copy 11 14 'T?d! at  90 deg (MeV)'
  hi/copy 11 15 'T?d! at 116 deg (MeV)'
  hi/copy 11 16 'T?d! at 151 deg (MeV)'
  hi/copy 11 81 'zero level'
  hi/copy 11 82 'dT in MeV'
  call hbarx(0)
*
* -- adjust zero levels, create dT histogram
  ve/cr zerol([hnbin]) r [hnbin]*[hzero]
  put/cont 81 zerol
  put/err  81 $sigma(0.8*zerol)
** ___ Reuse of "zerol" to read the bin mean T-values ___
  get/abs  82 zerol
  put/cont 82 $sigma(10.**(zerol+[halfbn])-10.**(zerol-[halfbn]))
  ve/del zerol
*
  cut 0 -
	 cut $1 nd=0.and.PDG=211  | pi+
	 cut $2 nd=0.and.PDG=-211 | pi-

*
*  opt zfl
*  swi z
  zone 3 2
* Vector of the center bins in Kinetic Energy for all spectra (the same)
  ve/cr lke([hnbin])
  get/absc 11 lke
* --- Loop over angles (secondaries are separate inside the loop) ---
  do i = 0 , 5
    k  = [i]+1
    k1 = [k]+10 | pi+
*    k2 = [k]+20 | pi-
    a=angle([k])
    arad =  $sigma([pi]*[a]/180.)
    message 'i=',[i],', adeg=',[a],', arad=',[arad]
    cut [k1] $1.and.abs(atan2(sqrt(px*px+py*py),pz)-[arad])<[danrad]
*    cut [k2] $2.and.abs(atan2(sqrt(px*px+py*py),pz)-[arad])<[danrad]
*    message 'cuts are defined k=',[k1],[k2],[k3],[k4],[k5],[k6]
    nt/pl 70.log10(E-m+0.001) $[k1] ! ! -[k1]
*    nt/pl 70.log10(E-m+0.001) $[k2] ! ! -[k2]
    domega = $sigma(2.*[pi]*(cos([arad]-[danrad])-cos([arad]+[danrad])))
    w = $sigma([cs]/[domega]/[nevts])
*    message 'selection is done dom=',[domega],', w=',[w],', cs=',[cs]
    div [k1] 82 [k1]  1. 1.
    add [k1] 81 [k1] [w] 1.
*    div [k2] 82 [k2]  1. 1.
*    add [k2] 81 [k2] [w] 1.
*    message 'histograms are waighted and added'
  enddo
  pi/del ' '
*
  opt zfl
* Draw & Create the file
  swi gz
* Create the output file only
*  swi z
*
*  opt vert
  size 24 18
  set xwin 0.0001
  set ywin 0.0001
*
*  model=lowen
*  model=chips
*  model=preco
*  model=berti
  model=binar
*  model=lepro
*
  title [targ]//'(p,[p]^+!)X, E='//[E]//'MeV, [q]=30,45,60,90,116,151^0! ('//[model]//')'
  clr
  zone 1 1
  set xval
  opt logy
  opt linx
  set dmod 1
  ***** M.K. ****
  opt date
  ***************
  opt tic
  opt nsta
  opt utit
  set errx 0.01
  set xlab 1.25
  ve/cr mrk(6) R 20 24 21 25 22 26
  ve/cr md(6) R 1 2 1 2 1 2
*  ve/cr mas(6) R 938.272 1875.61 2809.25 3727.4 
  f1=pi+
  f2=pi-
  title ' ' u
  exec ~/PTOOL/black
  set xval
  set yval 70
  xt=7
  ve/cr xm(1) R 5.
  ve/cr ym1(1) R .00000208
  ve/cr ym2(1) R .00000104
  ve/cr ym3(1) R .00000052
  ve/cr ym4(1) R .00000026
  ve/cr ym5(1) R .00000013
  ve/cr ym6(1) R .000000065
* --- Loop over pictures for each secondary ---
*  do j = 1 , 4
   j=1
*    if [j] = 2 THEN
      set xval 70
*    endif
*    if [j] = 3 THEN
*      set xval
*      set yval
*      hymin=1.001e-6
*      hymax=.04
*      yfnam=.02
*    endif
*    if [j] = 4 THEN
*      set xval 70
*    endif
    null [kxmin] [kxmax] [hymin] [hymax]
*    if [j] = 1 THEN
      h/symbols xm ym1 1 20 [ptsize]
      h/symbols xm ym2 1 24 [ptsize]
      h/symbols xm ym3 1 21 [ptsize]
      h/symbols xm ym4 1 25 [ptsize]
      h/symbols xm ym5 1 22 [ptsize]
      h/symbols xm ym6 1 26 [ptsize]
      itx [xt] .00000192 30
      itx [xt] .00000096 45
      itx [xt] .00000048 60
      itx [xt] .00000024 90
      itx [xt] .00000012 116
      itx [xt] .00000006 151
*    endif
*    if [j] = 4 then
      exec ~/PTOOL/black
*     atitle 'k = (p+T)/2 (MeV)       ^!' ' '
      atitle 'T (MeV)       ^!' ' '
*    endif
    title ' '
*    itx [xfnam] [yfnam] [f[j]]
    itx [xfnam] [yfnam] '[p]^+!'
*    if [j] = 1 then
      exec ~/PTOOL/black
      atitle ' ' 'd[s]/pdTd[W] (mb MeV^-2! sr^-1!) ^!'
      exec paramline 0.2 0.2
*    endif
*    exec ~/PTOOL/snotes 144. 1.0 '1.0' 0. [lgndsz] -40
*    exec ~/PTOOL/snotes 144. 0.1 '0.1' 0. [lgndsz] -40
*    m = mas([j])
    m = 139.57
    exec ~/PTOOL/black
    do i = 0 , 5
      k    = [i]+1
      jk   = [j]*10+[k]
      mk=mrk([k])
      mod=md([k])
      a=angle([k])
      set lwid 7.
**      fun/pl .11*EXP(-x/25) 30 250 S
      ve/copy $sigma(sqrt(t[j][a]*(2.*[m]+t[j][a]))) p[j][a]
      *h/errors $sigma(0.5*(p[j][a]+t[j][a])) $sigma(s[j][a]/p[j][a])
      h/errors t[j][a] $sigma(s[j][a]/p[j][a]) _
               ?  $sigma(d[j][a]/p[j][a]) 25 [mk] [ptsize] 0
      exec hsolid [j][k] [hfbin] [hnbin] [downcol] [m] [mod]
*     prepare the vector corresponding to the histogram
      ve/cr v[jk]([hnbin])
      get/cont [jk] v[jk]
    enddo
*   pip(j=1) (angles: 1=30, 2=45, 3=60, 4=90, 5=116, 6=151)
    ve/wri lke,v[j]1,v[j]2,v[j]3,v[j]4,v[j]5,v[j]6 p_[e]_[file]_[j]_[model].v _
      '1X,F9.6,6E10.3' 'OC'
*  enddo
*
  exec ~/PTOOL/black
  set xwin
  set ywin
  set xval
  set yval
  set errx
  set xlab
*
  exec prieps p_[e]_[file]_pi_[model]
  if [runn] = 0 then
    wait ' ' 2
***    shell lpr -P[pq] pipro_[file]_e[e]_[model].ps
***    shell xprint -P[pq] pipro_[file]_e[e]_[model].ps
  endif
*
stop:
*
return
