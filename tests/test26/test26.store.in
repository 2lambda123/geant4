/control/verbose 2
/run/verbose 1
/tracking/verbose 0
#
#  select EM PhysicsList
#
#/testem/phys/addPhysics  model
/testem/phys/addPhysics  standard
#
/testem/phys/WorldCuts  5.0 mm
/testem/phys/VertexCuts 0.01 mm
/testem/phys/MuonCuts  10.0 mm
/run/initialize
#
/gun/particle mu-
/gun/energy 1 GeV
/run/beamOn 100
/run/particle/storePhysicsTable physdata
#
/gun/particle pi+
/gun/energy 1 GeV
/run/beamOn 100
#
/gun/particle ion
/gun/ion      6 12 6 0
/gun/energy 1 GeV
/run/beamOn 100
