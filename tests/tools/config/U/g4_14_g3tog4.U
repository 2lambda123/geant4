?RCS:
?MAKE:g4wlib_build_g3tog4 g4w_use_g3tog4 :\
	 Myread Getfile cat rm test
?MAKE:	-pick add $@ %<
?S:g4osname:
?S:	This variable contains the value of the 'g4osname' symbol, which
?S:	used for building G4SYSTEM.
?S:.
?INIT:#INITIALIZE G3TOG4 OPTIONS
?INIT:g4wlib_build_g3tog4='n'
?INIT:g4wlib_use_g3tog4='n'

g4message="Enable build of the g3tog4 utility module?"
dflt=$g4wlib_build_g3tog4
rp=$g4message
. ./myread
gans=$ans

case $gans in
	n*|N*) 
		g4wlib_build_g3tog4=n
		g4w_use_g3tog4=n
		;;
	y*|Y*) 
		g4wlib_build_g3tog4=y
		g4w_use_g3tog4=y
		;;
esac

