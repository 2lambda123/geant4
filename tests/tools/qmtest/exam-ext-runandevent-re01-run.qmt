<?xml version="1.0"?>
<!DOCTYPE extension PUBLIC "-//QM/2.4/Extension//EN" "http://www.codesourcery.com/qm/dtds/2.4/-//qm/2.4/extension//en.dtd">
<extension class="Geant4Test.Geant4Test" kind="test">
  <argument name="target_group">
    <text>.*</text>
  </argument>
  <argument name="timeout">
    <integer>3000</integer>
  </argument>
  <argument name="stderr">
    <text>Warning : Region &lt;TrackerRegion&gt; does not have specific production cuts,
even though it appears in the current tracking world.
Default cuts are used for this region.
Warning : Region &lt;CalorimeterRegion&gt; does not have specific production cuts,
even though it appears in the current tracking world.
Default cuts are used for this region.</text>
  </argument>
  <argument name="stdout">
    <text/>
  </argument>
  <argument name="prerequisites">
    <set>
      <tuple><text>exam-ext-runandevent-re01-build</text><enumeral>PASS</enumeral></tuple>
    </set>
  </argument>
  <argument name="stdin">
    <text/>
  </argument>
  <argument name="exit_code">
    <integer>0</integer>
  </argument>
  <argument name="environment">
    <set/>
  </argument>
  <argument name="command">
    <text>cd $G4INSTALL/examples/extended/runAndEvent/RE01;$G4BIN/$G4SYSTEM/RE01 sample.in</text>
  </argument>
  <argument name="resources">
    <set/>
  </argument>
</extension>
