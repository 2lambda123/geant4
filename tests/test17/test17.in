#
# Macro file for the initialization phase of "test17" based on "TestEm6.cc"
#
# It creates the default geometry (simple absorber box )        
#
/control/verbose 2
/event/setverbose 0
#
/run/initialize
#
/gun/particle proton
#/gun/zvertex -0.16 mum
/gun/energy 500 keV
/tracking/verbose 0
/run/beamOn 1000
#
/gun/particle anti_proton
/run/beamOn 1000

#scaled energy
/gun/particle IonC12
/gun/energy 6000 keV
/run/beamOn 1000

