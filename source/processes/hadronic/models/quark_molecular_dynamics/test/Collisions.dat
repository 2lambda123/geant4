#
# General declarations
#
X	-> in	 1	NORMAL
X X	-> in,in 10	NORMAL

#
# Quark - decays
#
# Note: each hadron can decompose into quarks. But this decay mode is
# DIFFERENT from the hadronic decay and has to be forced "by hand".
# Therefore the fractions are given by NEGATIVE numbers!
#
S0	->	q,q'		-1	DECOMPOSITION

K*	->	q,s'		-1	DECOMPOSITION
Kbar*	->	q',s		-1	DECOMPOSITION

D*	->	q',c		-1	DECOMPOSITION
Dbar*	->	q,c'		-1	DECOMPOSITION
Ds*	->	s',c		-1	DECOMPOSITION
Dsbar*	->	s,c'		-1	DECOMPOSITION
R	->	q,q,q		-1	DECOMPOSITION
Y	->	q,q,s		-1	DECOMPOSITION
Yc	->	q,q,c		-1	DECOMPOSITION
#Ycs	->	q,s,c		-1	DECOMPOSITION
Xi*	->	q,s,s		-1	DECOMPOSITION
Omega*	->	s,s,s		-1	DECOMPOSITION

#
# Diquark decay:
#
# Diquarks are treated NOT like quarks, but like hadrons, since there
# is no other decay channel than decomposition!
#
DQq	->	q,q		1	NORMAL
DQs	->	q,s		1	NORMAL
ss_1	->	s,s		1	NORMAL

#
# String Fragmentation
