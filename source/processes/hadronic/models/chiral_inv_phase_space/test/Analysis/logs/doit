rm liste; rm liste.$2
grep "SECONDARIES info" $1 > liste
sed "s/(/ /g" liste >> liste@ ; mv liste@ liste
sed "s/)/ /g" liste >> liste@ ; mv liste@ liste
sed "s/,/ /g" liste >> liste@ ; mv liste@ liste
sed "s/SECONDARIES info//g" liste >> liste@ ; mv liste@ liste.p_$2
