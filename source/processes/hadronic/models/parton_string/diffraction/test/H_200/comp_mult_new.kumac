macro comp_mult
      IGSET chhe .3
      set *fon -130
      set *wid 2
      set yval 100.
      opt zfl
      v/del xtxt
      v/del ytxt
*      v/del ydata
      v/cre xtxt(2) r
      v/cre ytxt(1) r
      v/cre ydelta(1) r

      v/in xtxt 7. 7.2
      v/in ydelta 0.25

set xsiz 15.
set ysiz 15.
set ygti 1.2      
set GSIZ .35
Title 'pp "5# X, 200 GeV/c'
*---


*exec axis
v/in ytxt 3.5
zone 1 1
null 0 11 0 4  
exec data
exec axis

*---
v/in ytxt 10.
null 0 11 0 0.4 
exec data
exec axis
SET TANG 90
itx -1.  5. Mean Mulitiplicity 
 SET TANG 0
IGSET chhe .5
igset txal 20
*itx 6.5 9.7 'Diffractive String Model' 
igset txal 0
IGSET chhe .2
*itx 11. -1.0 'G.Folger'
return
