grep STRINGDUMP $1.liste > $1.secondaries.liste
sed "s/STRINGDUMP //g" $1.secondaries.liste > $1.secondaries.liste@
mv $1.secondaries.liste@ $1.secondaries.liste
sed "s/(//g" $1.secondaries.liste > $1.secondaries.liste@
mv $1.secondaries.liste@ $1.secondaries.liste
sed "s/)//g" $1.secondaries.liste > $1.secondaries.liste@
mv $1.secondaries.liste@ $1.secondaries.liste
sed "s/,/ /g" $1.secondaries.liste > $1.secondaries.liste@
mv $1.secondaries.liste@ $1.secondaries.liste
sed "s/;/ /g" $1.secondaries.liste > $1.secondaries.liste@
mv $1.secondaries.liste@ $1.secondaries.liste
