grep DUMP $1.liste > $1.predecay.liste
sed "s/PREDECAYDUMP //g" $1.predecay.liste > $1.predecay.liste@
mv $1.predecay.liste@ $1.predecay.liste
sed "s/(//g" $1.predecay.liste > $1.predecay.liste@
mv $1.predecay.liste@ $1.predecay.liste
sed "s/)//g" $1.predecay.liste > $1.predecay.liste@
mv $1.predecay.liste@ $1.predecay.liste
sed "s/,/ /g" $1.predecay.liste > $1.predecay.liste@
mv $1.predecay.liste@ $1.predecay.liste
sed "s/;/ /g" $1.predecay.liste > $1.predecay.liste@
mv $1.predecay.liste@ $1.predecay.liste
