* trace on

ntot = 500

ve/del *
hi/del *
nt/cr 1 ' ' 6 ' ' 200000 ic ip px py pz ee
nt/read 1 [1]
1d 55 ' ' 50 [2] [3]
nt/pl 1.0.5*log((ee+pz)/(ee-pz+0.0001)) ip=-211 ! -55

1d 57 ' ' 50 [2] [3]
nt/pl 1.0.5*log((ee+pz)/(ee-pz+0.0001)) ip=211 ! -57

1d 59 ' ' 50 0 2
nt/pl 1.(px*px+py*py)/1000./1000. ip=-211 ! -59

1d 61 ' ' 50 0 2
nt/pl 1.(px*px+py*py)/1000./1000. ip=211 ! -61

de = ([3]-[2])/50.
norm = 1./[de]/[ntot]

* norm eta spectra
add 55 55 56 0.0 [norm]
add 57 57 58 0.0 [norm]
ve/cr y56(50) r 50*0.
ve/cr y58(50) r 50*0.
ve/cr x56(50) r 50*0.
ve/cr x58(50) r 50*0.
ve/cr ey56(50) r 50*0.
ve/cr ey58(50) r 50*0.
ve/cr ex56(50) r 50*0.
ve/cr ex58(50) r 50*0.

get_vect/contents 56 y56
get_vect/ABSCISSA 56 x56
sigma ey56 = sqrt(y56)*sqrt([norm])
sigma ex56 = x56/1000
get_vect/contents 58 y58
get_vect/ABSCISSA 58 x58
sigma ey58 = sqrt(y58)*sqrt([norm])
sigma ex58 = x58/1000.

* norm pt^2 spectra
de = 2./50.
norm1 = 1./[de]/[ntot]
add 59 59 60 0.0 [norm1]
add 61 61 62 0.0 [norm1]
ve/cr y60(50) r 50*0.
ve/cr y62(50) r 50*0.
ve/cr x60(50) r 50*0.
ve/cr x62(50) r 50*0.
ve/cr ey60(50) r 50*0.
ve/cr ey62(50) r 50*0.
ve/cr ex60(50) r 50*0.
ve/cr ex62(50) r 50*0.

get_vect/contents 60 y60
get_vect/ABSCISSA 60 x60
sigma ey60 = sqrt(y60)*sqrt([norm1])
sigma ex60 = x60/1000
get_vect/contents 62 y62
get_vect/ABSCISSA 62 x62
sigma ey62 = sqrt(y62)*sqrt([norm1])
sigma ex62 = x62/1000.
ve/pri y60
ve/pri y62
