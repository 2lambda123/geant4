set mscf 0.3
nt/pl 2.sig%(sqrt(2.*elab)) elab>5.and.elab<1000 ! -s
nt/pl 21.sig%(sqrt(2.*elab)) elab>5.and.elab<1000 ! -s
atit 'S "M#GeV^2!"n#' '[s]?tot! "m#mb"n#'

goto end

