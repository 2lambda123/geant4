/histo/smooth 3
/histo/smooth 4
/histo/save 9
/histo/save 10
/histo/save 11
/histo/save 12
/histo/readOptions 9 options/HistoOpt1.txt
/histo/readOptions 10 options/HistoOpt1.txt
/histo/readOptions 11 options/HistoOpt1.txt
/histo/add 9 histos/beamaxis.paw 82
/histo/add 9 histos/beamaxis.paw 83
