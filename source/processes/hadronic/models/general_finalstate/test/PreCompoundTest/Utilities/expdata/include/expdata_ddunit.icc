inline expdata_ddunit::expdata_ddunit() : 
  angle(-1.0)
{
}

inline expdata_ddunit::expdata_ddunit(const expdata_ddunit & val) : 
  expdata_unit(val), angle(val.angle) 
{
}

inline expdata_ddunit::~expdata_ddunit()
{
}

inline const expdata_ddunit & expdata_ddunit::operator=(const expdata_ddunit & val) 
{
  expdata_unit::operator=(val);
  angle = val.angle;
  return *this;
}

inline expdata_ddunit::expdata_ddunit(const Int_t n, const Double_t ang, 
				      const Double_t * e, const Double_t * ee,
				      const Double_t * d, const Double_t * ed) :
  expdata_unit(n,e,ee,d,ed), angle(ang) 
{
}

inline expdata_ddunit::expdata_ddunit(const Int_t n, const Double_t ang, 
				      const Double_t * e, const Double_t * eel, const Double_t * eeh,
				      const Double_t * d, const Double_t * edl, const Double_t * edh) :
  expdata_unit(n,e,eel,eeh,d,edl,edh),  angle(ang)  
{
}

inline Double_t expdata_ddunit::GetAngle() const 
{ 
  return angle; 
}

inline Double_t expdata_ddunit::GetEnergy(const Int_t i) const 
{ 
  return this->GetDataX(i);
}

inline Double_t expdata_ddunit::GetEnergyError(const Int_t i) const 
{
  return this->GetErrorX(i);
}
  
inline Double_t expdata_ddunit::GetDifferentialXS(const Int_t i) const
{ 
  return this->GetDataY(i); 
}

inline Double_t expdata_ddunit::GetDifferentialXSError(const Int_t i) const 
{ 
  return this->GetErrorY(i); 
}

inline void expdata_ddunit::ShowYourSelf() const
{
  std::ostringstream os;
  this->ShowYourSelf(os);
  std::cout << os.rdbuf();
  return;
}
